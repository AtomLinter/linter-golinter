{
  "name": "linter-golinter",
  "main": "lib/init.coffee",
  "version": "1.2.0",
  "description": "Check Go files with golint tool",
  "keywords": [
    "go",
    "golang",
    "linter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AtomLinter/linter-golinter.git"
  },
  "bugs": {
    "url": "https://github.com/AtomLinter/linter-golinter/issues"
  },
  "homepage": "https://github.com/AtomLinter/linter-golinter",
  "license": "MIT",
  "engines": {
    "atom": ">=0.174.0"
  },
  "dependencies": {
    "atom-linter": "^4.5.0",
    "atom-package-deps": "^4.0.1"
  },
  "devDependencies": {
    "coffeelint": "^1.14.2",
    "eslint": "^1.10.3",
    "babel-eslint": "^4.1.8",
    "eslint-config-airbnb": "^5.0.0"
  },
  "eslintConfig": {
    "rules": {
      "comma-dangle": [
        2,
        "never"
      ],
      "no-console": 0
    },
    "extends": "airbnb/base",
    "parser": "babel-eslint",
    "globals": {
      "atom": true
    },
    "env": {
      "es6": true,
      "node": true
    }
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
