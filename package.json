{
  "name": "linter-golinter",
  "main": "lib/init.coffee",
  "version": "1.2.0",
  "description": "Check Go files with golint tool",
  "keywords": [
    "go",
    "golang",
    "linter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AtomLinter/linter-golinter.git"
  },
  "bugs": {
    "url": "https://github.com/AtomLinter/linter-golinter/issues"
  },
  "homepage": "https://github.com/AtomLinter/linter-golinter",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "lint": "coffeelint lib && eslint spec",
    "test": "apm test"
  },
  "dependencies": {
    "atom-linter": "^8.0.0",
    "atom-package-deps": "^4.0.1"
  },
  "devDependencies": {
    "coffeelint": "^1.14.2",
    "eslint": "^2.9.0",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.10.1"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "globals": {
      "atom": true
    },
    "env": {
      "es6": true,
      "node": true
    }
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
